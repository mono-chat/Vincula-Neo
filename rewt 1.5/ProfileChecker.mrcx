    if (<font class=viewName> isin $1-) {
      echo -a 14            Username » $msn.decode($mid($mid($1-,$calc($pos($1-,<font class=viewName>,1) + $len(<font class=viewName>)),$len($1-)),1,$calc($pos($mid($1-,$calc($pos($1-,<font class=viewName>,1) + $len(<font class=viewName>)),$len($1-)),</font>,1) - 1)))
